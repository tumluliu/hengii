#!/bin/bash

# check if stdin contains specifical message

result="true"
ACTUAL=$(cat)

for arg in "$*"
do
	echo $ACTUAL | grep -q "$arg"
	if [ ! $? -eq 0 ]; then
		result="false"
		miss="$arg"
		break
	fi
done

# show result
if [ $result == "false" ]; then
	echo "false; not contain $miss"
else
	echo true
fi
